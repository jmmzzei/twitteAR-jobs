            <button id="theme">Dark</button>
        </div>
        
    </nav>
    <div class="search"></div>
    <div class="wrapper">

        <!-- <%include('./partials/home')%> -->

        <% for (var i = 0; i<arr.length; i++) { %>
        <div class="card">
            
            <div class="card-header">
                <p class="name"><%= arr[i].name%></p>
                <p class="time"><%= arr[i].timestamp%></p>
            </div>
            
            <div class="card-body">
                <!-- <span style="opacity: 0.5"><%= arr[i].text.mentions%> </span> -->
                <p><%- arr[i].text.full%></p>
            </div>
            <div class="card-footer">
                <a class="username" target="_blank" href="https://twitter.com/<%= arr[i].username%>">@<%= arr[i].username%></a>
                <a target="_blank" href="https://twitter.com/<%= arr[i].username%>/status/<%= arr[i].id%>">ir al tweet</a>
            </div>
        </div>
        <%}%>
    </div>

<script src="../js/main.js"></script>
</body>
</html>